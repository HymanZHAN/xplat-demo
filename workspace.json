          ],
          "options": {
            "jestConfig": "libs/xplat/ionic/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "xplat-ionic-features": {
      "root": "libs/xplat/ionic/features",
      "sourceRoot": "libs/xplat/ionic/features/src",
      "projectType": "library",
      "targets": {
