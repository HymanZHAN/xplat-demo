    "xplat-web-scss": {
      "root": "libs/xplat/web/scss",
      "sourceRoot": "libs/xplat/web/scss/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/xplat/web/scss/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/xplat/web/scss"
