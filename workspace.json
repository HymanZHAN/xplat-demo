{
  "version": 2,
  "projects": {
    "xplat-core": {
      "root": "libs/xplat/core",
      "sourceRoot": "libs/xplat/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/xplat/core/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/xplat/core"
          ],
          "options": {
            "jestConfig": "libs/xplat/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
