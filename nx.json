{
  "npmScope": "xplat-demo",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "xplat-core": { "tags": [] },
    "xplat-features": { "tags": [] },
    "xplat-scss": { "tags": [] },
    "xplat-utils": { "tags": [] },
    "xplat-web-core": { "tags": [] },
    "xplat-web-features": { "tags": [] },
    "xplat-web-scss": { "tags": [] },
    "web-tasks": { "tags": [] },
    "web-tasks-e2e": { "tags": [], "implicitDependencies": ["web-tasks"] },
    "xplat-ionic-core": { "tags": [] },
    "xplat-ionic-features": { "tags": [] },
    "xplat-ionic-scss": { "tags": [] },
    "ionic-tasks": { "tags": [] },
    "ui-lib": { "tags": [] },
    "ui-lib-angular": { "tags": [] }
  }
}
