    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "build.ionic.tasks": "nx build ionic-tasks",
    "prepare.ionic.tasks": "npm run clean && npm run clean.ionic.tasks && npm run build.ionic.tasks",
    "prepare.ionic.tasks.ios": "npm run prepare.ionic.tasks && cd apps/ionic-tasks && npx rimraf -- ios && npm run cap.add.ios",
    "prepare.ionic.tasks.android": "npm run prepare.ionic.tasks && cd apps/ionic-tasks && npx rimraf -- android && npm run cap.add.android",
    "open.ionic.tasks.ios": "cd apps/ionic-tasks && npm run cap.ios",
    "open.ionic.tasks.android": "cd apps/ionic-tasks && npm run cap.android",
    "sync.ionic.tasks": "cd apps/ionic-tasks && npm run cap.copy",
    "clean.ionic.tasks": "cd apps/ionic-tasks && npx rimraf -- hooks node_modules platforms www plugins package-lock.json && npm i && rimraf -- package-lock.json"
  },
